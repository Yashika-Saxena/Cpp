#include<iostream>
using namespace std;
int main(){
    int n , i;
    int arr[n];
    cout<< "Enter the no of array element: \n";
    cin >> n ;
    cout<< "\nEnter the array element: \n";
    for(i=0; i< n; i++)
    {
        cin >> arr[i];
    }
    cout<< "The array element: \n";
    for(i=0; i< n; i++)
    {
        cout << arr[i] <<endl;
    }
}

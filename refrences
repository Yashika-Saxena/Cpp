#include <iostream>
#include <string>
using namespace std;

int main() {
  string food = "Pizza";
  string &meal= food;

  cout << &food<<endl;
  cout<< &meal;
  return 0;
}


// output
0x7ffd8f367650
0x7ffd8f367670


#include <iostream>
#include <string>
using namespace std;

int main() {
  string food = "Pizza";
  string &meal = food;

  cout << food << "\n";
  cout << meal << "\n";
  return 0;
}

//output
Pizza
Pizza
